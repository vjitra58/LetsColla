{
    "version": 2,
    "builds": [
        {
            "src": "./server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/public/index.html"
        },
         {
            "src": "/room.html/(.*)",
            "dest": "/public/room.html"
        }
    ]
}